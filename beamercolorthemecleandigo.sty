\mode<presentation>
\ProvidesPackage{beamercolorthemecleandigo}[Cleandigo color theme]

%
\definecolor{md-black}{HTML}{231f20}
\definecolor{md-white}{HTML}{e3e4e5}

% material design color
\definecolor{md-primary}{HTML}{1a237e}
\definecolor{md-accent}{HTML}{d81b60}

% color palettes
\setbeamercolor{palette primary}{fg=md-primary}
\setbeamercolor{palette secondary}{fg=md-accent}
% Title page, part, section, subsection
\setbeamercolor{titlelike}{fg=white}
\setbeamercolor{title}{fg=md-primary}
\setbeamercolor{partname}{fg=md-primary}
%% table of contents
\setbeamercolor{section in toc}{fg=md-primary}
\setbeamercolor{subsection in toc}{fg=md-black}
\setbeamercolor{section number projected}{fg=md-black}
\setbeamercolor{subsection number projected}{fg=md-black}
\setbeamercolor{subsubsection number projected}{fg=md-black}
\setbeamercolor{item}{fg=md-primary, bg=md-accent}
%% Figures and table
\setbeamercolor{caption}{fg=md-black}
\setbeamercolor{caption name}{fg=md-primary}
%% Elements of a frame
\setbeamercolor{framesubtitle}{bg=md-primary, fg=md-white}
\setbeamercolor{frametitle}{bg=md-primary, fg=md-white}
\setbeamercolor{page number in head/foot}{bg=md-accent, fg=md-white}
\setbeamercolor{section in head/foot}{bg=md-primary, fg=md-white}
\setbeamercolor{subsection in head/foot}{bg=md-primary, fg=md-white}
%% Blocks
\setbeamercolor{block title}{fg=md-primary}
\mode<all>
